{
  "root": true,
  "parser": "@typescript-eslint/parser",
  
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/semi": "warn",
    "quotes": [
      "error",
      "double"
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2
      }
    ]
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ]
}